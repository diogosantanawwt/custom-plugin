<?xml version='1.0' encoding='utf-8'?>
<plugin id="cordova-plugin-custom-venmo" version="0.0.1"
    xmlns="http://apache.org/cordova/ns/plugins/1.0"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <name>CustomVenmoPlugin</name>
    <js-module name="CustomVenmoPlugin" src="www/CustomVenmoPlugin.js">
        <clobbers target="cordova.plugins.CustomVenmoPlugin" />
    </js-module>
    <platform name="android">
        <config-file parent="/*" target="res/xml/config.xml">
            <feature name="CustomVenmoPlugin">
                <param name="android-package" value="cordova-plugin-custom-venmo.CustomVenmoPlugin" />
            </feature>
        </config-file>
        <config-file parent="/*" target="AndroidManifest.xml"></config-file>
        <source-file src="src/android/CustomVenmoPlugin.java" target-dir="src/cordova-plugin-custom-venmo/CustomVenmoPlugin" />
    </platform>
</plugin>